<div class="page-header">
  <div class="ui breadcrumb">
    <%= link_to 'Все курсы', courses_path(@period.group.course) %>
    <div class="divider"> / </div>
    <%= link_to  @period.course.name, show_course_index_path(@period.group.course, resource: 2) %>
    <div class="divider"> / </div>
    <%= link_to  @period.group.name, show_group_index_path(@period.group, resource: 2) %>
    <div class="divider"> / </div>
    <%= link_to @period.title, period_path(@period) %>
    <div class="divider"> / </div>
    <div class="active section"><%= @material.title %></div>
  </div>
  <h5 class="text-center"><%= @material.title %> (<%= @material.element_type %>)</h5>
</div>
<div class="embedded-editor">
  <div class="ui segment no-border">
    <%= link_to 'Распечатать', ' ' , onclick: "printpage()", class: 'btn btn-default print pull-right' %>
    <%= raw@material.content %>
  </div>
</div>